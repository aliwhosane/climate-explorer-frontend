{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
  },
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-var": 0,
    "vars-on-top": 1,
    "max-len": 1,
    "prefer-arrow-callback": 0,
    "one-var": 1,
    "func-names": 0,
    "object-shorthand": 0,
  }
}
