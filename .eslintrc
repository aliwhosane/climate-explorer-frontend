{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "jest": true,
    "jquery": true,
  },
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "globals": {
    "CE_BACKEND_URL": false,
    "CE_ENSEMBLE_NAME": false,
    "TILECACHE_URL": false,
    "NCWMS_URL": false,
    "L"
  },
  "rules": {
    "no-var": 0,
    "vars-on-top": 1,
    "max-len": 1,
    "prefer-arrow-callback": 0,
    "one-var": 1,
    "func-names": 0,
    "object-shorthand": 0,
    "prefer-const": 0,
    "react/prefer-es6-class": 0,
    "guard-for-in": 0,
    "react/jsx-no-bind": 1,
    "react/sort-comp": 1,
    "prefer-template": 0,
  }
}
